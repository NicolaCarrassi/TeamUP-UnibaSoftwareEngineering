
nav{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    transition: 0.6s;
    padding: 40px 100px;
    z-index: 10000;
}

.cc {
    margin-bottom: 50px;
}


nav.sticky {
    background-color: #ffffff;
    padding: 10px 100px;
}




